-language standard

-alias portA 0xFFF

#/
Fibonacci series generator

watch 0xFFF for the output
/#


start:
    load var.a
    store $portA$
    addm var.b
    store var.temp
    load var.a
    store var.b
    load var.temp
    store var.a

    # only 10 iterations
    load var.count
    sub RA 1
    store var.count
    jumpnz start
exit:
    jump exit

var:
    .data -a 1
    .data -b 1
    .data -temp 0
    .data -count 10